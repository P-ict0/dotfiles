"custom/spotify": {
  "exec": "/usr/bin/python3 $HOME/scripts/mediaplayer.py --player spotify",
  "format": "{}",
  "return-type": "json",
  "on-click": "/usr/bin/playerctl -p spotify play-pause || /usr/bin/spotify-launcher &",
  "on-scroll-up": "/usr/bin/playerctl next",
  "on-scroll-down": "/usr/bin/playerctl previous"
},
