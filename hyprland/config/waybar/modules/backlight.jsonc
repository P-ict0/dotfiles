"backlight": {
  "device": "intel_backlight",
  "rotate": ${r_deg},
  "format": "{icon} {percent}%",
  "format-icons": [
    "",
    "",
    "",
    "",
    "",
    "",
    "",
    "",
    ""
  ],
  "on-scroll-up": "brightnessctl -d intel_backlight set 1%+ | brightnessctl -d nvidia_0 set 1%+",
  "on-scroll-down": "brightnessctl -d intel_backlight set 1%- | brightnessctl -d nvidia_0 set 1%-",
  "min-length": 6
},
